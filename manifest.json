{
  "name": "Geo Stats",
  "description": "",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://*.geoguessr.com/api/v3/games/*'"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icon/48.png",
    "96": "icon/96.png"
  }
}